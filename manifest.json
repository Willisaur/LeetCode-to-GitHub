{
  "name": "Solution Saver - Save LeetCode solutions to GitHub",
  "description": "Automatically upload your LeetCode solutions to GitHub.",
  "version": "3.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./icons/L2G_128.png"
  },
  "options_page": "./options/options.html",
  "icons": {
    "128": "./icons/L2G_128.png",
    "16": "./icons/L2G_16.png"
  },
  "permissions": [
    "storage",
    "webRequest"
  ],
  "host_permissions": [
    "https://api.github.com/*",
    "https://leetcode.com/graphql/",
    "https://leetcode.com/problems/*/submit/",
    "https://leetcode.com/submissions/detail/*/check/"
  ],
  "background": {
    "service_worker": "service-worker.js"
  }
}
