{
  "name": "Submission Saver - Save LeetCode solutions to GitHub",
  "description": "Automatically upload your LeetCode solutions to GitHub",
  "version": "3.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./icons/128.png"
  },
  "icons": {
    "128": "icons/128.png",
    "16": "icons/16.png"
  },
  "permissions": [
    "storage",
    "webRequest"
  ],
  "host_permissions": [
    "https://api.github.com/*",
    "https://github.com/login*",
    "https://leetcode.com/graphql/",
    "https://leetcode.com/problems/*/submit/",
    "https://leetcode.com/submissions/detail/*/check/"
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "key": ""
}